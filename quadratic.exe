#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int a=2;
	int b=3;
	int c=4;
	int x=(-b-sqrt(b*b-4*a*c))/(2*a);
	int y=(-b+sqrt(b*b-4*a*c))/(2*a);
	cout<<"x="<<x<<"y="<<y;
	
